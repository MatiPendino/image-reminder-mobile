{
  "expo": {
    "name": "image-reminder-mobile",
    "slug": "photo-reminder",
    "scheme": "image-reminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#cfedf7"
    },
    "ios": {
      "supportsTablet": true
    },
